{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",

    "parameters": {

    },

    "resources": [
    { // vnet
        "name": "vnet",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2021-04-01",
        "properties": {
            "mode": "Incremental",
            "templateLink": {
                "relativePath": "artifacts/vnet.json"
            },
            "parameters": {
                "vnetName": {
                    "value": "VNet1"
                },
                "vnetAddressPrefix": {
                    "value": "10.0.0.0/16"
                },
                "subnet1Prefix": {
                    "value": "10.0.0.0/24"
                },
                "subnet1Name": {
                    "value": "Subnet1"
                },
                "subnet2Prefix": {
                    "value": "10.0.1.0/24"
                },
                "subnet2Name": {
                    "value": "Subnet2"
                }
            }
        }
    },
    { // log analytics
        "name": "logAnalytics",
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2021-04-01",
        "properties": {
            "mode": "Incremental",
            "templateLink": {
                "relativePath": "artifacts/logAnalytics.json"
            },
            "parameters": {
                "workspaceName": {
                    "value": "laMyWorkspace"
                },
                "sku": {
                    "value": "pergb2018"
                },
                "resourcePermissions": {
                    "value": true
                },
                "heartbeatTableRetention": {
                    "value": 30
                }
            }
        }
    }

    ]
    
  }